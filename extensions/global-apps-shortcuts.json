{
  "name": "global-apps-shortcuts",
  "version": "1.0.0",
  "description": "Global Popular apps shortcuts",
  "main": [
    {
      "id": "youtube-search",
      "required": [
        "com.google.android.youtube",
        "app.revanced.android.youtube"
      ],
      "name": "Youtube Search",
      "description": "Youtube Search",
      "type": "scheme",
      "uri": "vnd.youtube://results",
      "keywords": [
        "youtube",
        "ytb",
        "search"
      ]
    },
    {
      "id": "instagram-search",
      "required": [
        "com.instagram.android"
      ],
      "name": "Instagram Search",
      "description": "Instagram Search",
      "type": "scheme",
      "uri": "instagram://search",
      "keywords": [
        "instagram",
        "ig",
        "search"
      ]
    },
    {
      "id": "chatgpt-voice-assistant",
      "required": [
        "com.openai.chatgpt"
      ],
      "name": "Voice Assistant",
      "description": "ChatGPT Voice Assistant",
      "type": "intent",
      "data": {
        "package": "com.openai.chatgpt",
        "class": "com.openai.voice.assistant.AssistantActivity"
      },
      "keywords": [
        "ai",
        "chatgpt",
        "assistant",
        "voice"
      ]
    },
    {
      "id": "google-photos-search",
      "required": [
        "com.google.android.apps.photos"
      ],
      "name": "Photos Search",
      "description": "Google Photos Search",
      "type": "scheme",
      "uri": "https://photos.google.com/search",
      "keywords": [
        "google photos",
        "gallery",
        "search"
      ]
    },
    {
      "id": "google-photos-library",
      "required": [
        "com.google.android.apps.photos"
      ],
      "name": "Photos Library",
      "description": "Google Photos Library",
      "type": "scheme",
      "uri": "https://photos.google.com/search/Library",
      "keywords": [
        "google photos",
        "gallery",
        "library"
      ]
    },
    {
      "id": "google-photos-favorites",
      "required": [
        "com.google.android.apps.photos"
      ],
      "name": "Photos Favorites",
      "description": "Google Photos Favorites",
      "type": "scheme",
      "uri": "https://photos.google.com/search/Favorites",
      "keywords": [
        "google photos",
        "gallery",
        "favorites"
      ]
    },
    {
      "id": "google-photos-trash",
      "required": [
        "com.google.android.apps.photos"
      ],
      "name": "Photos Trash",
      "description": "Google Photos Trash",
      "type": "scheme",
      "uri": "https://photos.google.com/search/Trash",
      "keywords": [
        "google photos",
        "gallery",
        "trash"
      ]
    },
    {
      "id": "google-photos-archive",
      "required": [
        "com.google.android.apps.photos"
      ],
      "name": "Photos Archive",
      "description": "Google Photos Archive",
      "type": "scheme",
      "uri": "https://photos.google.com/search/Archive",
      "keywords": [
        "google photos",
        "gallery",
        "archive"
      ]
    }
  ]
}